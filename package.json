{
  "name": "Pre",
  "version": "0.0.1",
  "description": "A webapp for pitch events",
  "productName": "Pre App",
  "author": "Sarbjeet S. Rayat",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "postinstall": "quasar prepare",
    "build:dev": "quasar build -m spa --mode development",
    "build:prod": "quasar build -m spa --mode production",
    "deploy:dev": "firebase use dev && npm run build:dev && firebase deploy --only hosting:dev",
    "deploy:prod": "firebase use prod && npm run build:prod && firebase deploy --only hosting:prod"
  },
  "dependencies": {
    "@dataconnect/generated": "file:src/dataconnect-generated",
    "@quasar/extras": "^1.16.4",
    "axios": "^1.2.1",
    "firebase": "^12.4.0",
    "pinia": "^3.0.1",
    "quasar": "^2.16.0",
    "vue": "^3.5.22",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@quasar/app-vite": "^2.1.0",
    "@vue/eslint-config-prettier": "^10.1.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.4.0",
    "postcss": "^8.4.14",
    "prettier": "^3.3.3",
    "vite-plugin-checker": "^0.11.0",
    "vue-eslint-parser": "^10.2.0"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
